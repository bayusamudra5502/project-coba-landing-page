    "version": "0.2.0",
    "configurations": [
        {
            "type": "firefox",
            "request": "launch",
            "reAttach": true,
            "name": "Debug di Firefox",
            "url": "http://localhost:5500",
            "webRoot": "${workspaceFolder}"
        },
        {
            "type": "pwa-chrome",
            "request": "launch",
            "name": "Debug di Chrome",
            "url": "http://localhost:5500",
            "webRoot": "${workspaceFolder}",
            "runtimeExecutable": "/snap/bin/chromium",

        }
    ]
}